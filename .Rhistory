import pandas as pd
import pandas as pd
library(reticulate)
version <- "3.9.6"
intall_python (version = version)
install_python (version = version)
virtualenv_create ("my_python", python_version = version)
use_virtualenv("my_python", required = TRUE)
virtualenv_install(envname = "my_python", "pandas", ignore_installed = FALSE, pip_options = character())
reticulate::repl_python()
